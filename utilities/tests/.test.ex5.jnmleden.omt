# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: LEMS_NML2_Ex5_DetCell.xml
engine: jNeuroML_EDEN
mep: .test.ex5.mep
experiments:
  v:
    observables:
      spike times:
        file:
          path: ex5_v.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
          threshold: 0
        tolerance: 0.001963746223565029
