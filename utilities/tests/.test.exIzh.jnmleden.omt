# Script for running automated tests on OSB using Travis-CI, see https://github.com/OpenSourceBrain/osb-model-validation

target: LEMS_2007One.xml
engine: jNeuroML_EDEN
mep: .test.exIzh.mep
experiments:
  v:
    observables:
      spike times:
        file:
          path: exIzh.dat
          columns: [0,1]
          scaling: [1000, 1000]
        spike detection:
          method: threshold
        tolerance: 0.00014888369126089628
